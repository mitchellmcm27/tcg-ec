<?xml version='1.0' encoding='utf-8'?>
<reaction_options>
  <name name="AlAlO3_all_highp_slb_rx"/>
  <database>
    <string_value lines="1">file://../database/tcg_slb_database.tar.gz</string_value>
  </database>
  <reference>
    <string_value lines="1">generate_reactions</string_value>
  </reference>
  <phase name="AlAkimotoite_slb_ph"/>
  <phase name="AlPerovskite_slb_ph"/>
  <phase name="AlPostPerovskite_slb_ph"/>
  <reaction_rate name="R">
    <variable name="T">
      <rank name="Scalar"/>
      <units>K</units>
    </variable>
    <variable name="P">
      <rank name="Scalar"/>
      <units>bar</units>
    </variable>
    <variable name="C">
      <rank name="Matrix">
        <n_rows>N</n_rows>
        <n_cols>Kmax</n_cols>
      </rank>
      <units>None</units>
    </variable>
    <variable name="Phi">
      <rank name="Vector">
        <size>N</size>
      </rank>
      <units>%</units>
    </variable>
    <function name="A">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
        <variable name="C">
          <rank name="Matrix">
            <n_rows>N</n_rows>
            <n_cols>Kmax</n_cols>
          </rank>
          <units>None</units>
        </variable>
      </type>
    </function>
    <function name="rho">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
      </type>
    </function>
  </reaction_rate>
  <global_parameters>
    <parameter name="T0">
      <rank name="Scalar">
        <value>
          <real_value rank="0">2000</real_value>
        </value>
        <units>
          <string_value lines="1">'K'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="R">
      <rank name="Scalar">
        <value>
          <real_value rank="0">8.31446261815324</real_value>
        </value>
        <units>
          <string_value lines="1">'J/mol/K'</string_value>
        </units>
      </rank>
    </parameter>
  </global_parameters>
  <reaction name="AlAk_AlPe">
    <reactant name="AlAk_AlAk" phase="AlAkimotoite_slb_ph" endmember="AlAkimotoite_slb_em"/>
    <product name="AlPe_AlPe" phase="AlPerovskite_slb_ph" endmember="AlPerovskite_slb_em"/>
    <expression>
      <string_value type="code" language="python" lines="20">AlAk_AlPe = (
Piecewise((exp(-T0/T)*A[0, 0]*Phi[0, 0]/(R*T), A[0, 0] &gt;= 0),
(exp(-T0/T)*A[0, 0]*Phi[1, 0]/(R*T), True))
)</string_value>
    </expression>
  </reaction>
  <reaction name="AlAk_AlPo">
    <reactant name="AlAk_AlAk" phase="AlAkimotoite_slb_ph" endmember="AlAkimotoite_slb_em"/>
    <product name="AlPo_AlPo" phase="AlPostPerovskite_slb_ph" endmember="AlPostPerovskite_slb_em"/>
    <expression>
      <string_value type="code" language="python" lines="20">AlAk_AlPo = (
Piecewise((exp(-T0/T)*A[1, 0]*Phi[0, 0]/(R*T), A[1, 0] &gt;= 0),
(exp(-T0/T)*A[1, 0]*Phi[2, 0]/(R*T), True))
)</string_value>
    </expression>
  </reaction>
  <reaction name="AlPe_AlPo">
    <reactant name="AlPe_AlPe" phase="AlPerovskite_slb_ph" endmember="AlPerovskite_slb_em"/>
    <product name="AlPo_AlPo" phase="AlPostPerovskite_slb_ph" endmember="AlPostPerovskite_slb_em"/>
    <expression>
      <string_value type="code" language="python" lines="20">AlPe_AlPo = (
Piecewise((exp(-T0/T)*A[2, 0]*Phi[1, 0]/(R*T), A[2, 0] &gt;= 0),
(exp(-T0/T)*A[2, 0]*Phi[2, 0]/(R*T), True))
)</string_value>
    </expression>
  </reaction>
</reaction_options>

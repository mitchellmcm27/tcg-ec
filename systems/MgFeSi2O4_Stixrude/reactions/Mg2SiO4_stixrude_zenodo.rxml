<?xml version='1.0' encoding='utf-8'?>
<reaction_options>
  <name name="Mg2SiO4_stixrude"/>
  <database>
    <string_value lines="1">https://zenodo.org/record/6350797/files/MgFeSi2O4_Stixrude.tar.gz</string_value>
  </database>
  <reference>
    <string_value lines="1">Thermocodegen-v0.6/share/thermocodegen/examples/Systems/MgFeSiO2_stixrude/notebooks/Generate_reactions.ipynb</string_value>
  </reference>
  <phase name="Forsterite"/>
  <phase name="MgWadsleyite"/>
  <phase name="MgRingwoodite"/>
  <reaction_rate name="R">
    <variable name="T">
      <rank name="Scalar"/>
      <units>K</units>
    </variable>
    <variable name="P">
      <rank name="Scalar"/>
      <units>bar</units>
    </variable>
    <variable name="C">
      <rank name="Matrix">
        <n_rows>N</n_rows>
        <n_cols>Kmax</n_cols>
      </rank>
      <units>None</units>
    </variable>
    <variable name="Phi">
      <rank name="Vector">
        <size>N</size>
      </rank>
      <units>%</units>
    </variable>
    <function name="A">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
        <variable name="C">
          <rank name="Matrix">
            <n_rows>N</n_rows>
            <n_cols>Kmax</n_cols>
          </rank>
          <units>None</units>
        </variable>
      </type>
    </function>
    <function name="rho">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
      </type>
    </function>
  </reaction_rate>
  <global_parameters>
    <parameter name="T0">
      <rank name="Scalar">
        <value>
          <real_value rank="0">2000</real_value>
        </value>
        <units>
          <string_value lines="1">'K'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="R">
      <rank name="Scalar">
        <value>
          <real_value rank="0">8.314</real_value>
        </value>
        <units>
          <string_value lines="1">'J/(mol K)'</string_value>
        </units>
      </rank>
    </parameter>
  </global_parameters>
  <reaction name="Fo_Wa">
    <reactant name="Fo_Fo" phase="Forsterite" endmember="Forsterite_stixrude"/>
    <product name="MgWad_Wad" phase="MgWadsleyite" endmember="MgWadsleyite_stixrude"/>
    <expression>
      <string_value type="code" language="python" lines="20">Fo_Wa = (
Piecewise((exp(-T0/T)*A[0, 0]*Phi[0, 0]/(R*T), A[0, 0] &gt;= 0),
(exp(-T0/T)*A[0, 0]*Phi[1, 0]/(R*T), True))
)</string_value>
    </expression>
  </reaction>
  <reaction name="Wa_Ri">
    <reactant name="MgWad_Wad" phase="MgWadsleyite" endmember="MgWadsleyite_stixrude"/>
    <product name="MgRi_Ri" phase="MgRingwoodite" endmember="MgRingwoodite_stixrude"/>
    <expression>
      <string_value type="code" language="python" lines="20">Wa_Ri = (
Piecewise((exp(-T0/T)*A[1, 0]*Phi[1, 0]/(R*T), A[1, 0] &gt;= 0),
(exp(-T0/T)*A[1, 0]*Phi[2, 0]/(R*T), True))
)</string_value>
    </expression>
  </reaction>
</reaction_options>

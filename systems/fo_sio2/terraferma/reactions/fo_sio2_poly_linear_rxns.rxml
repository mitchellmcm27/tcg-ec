<?xml version='1.0' encoding='utf-8'?>
<reaction_options>
  <name name="fo_sio2_poly_linear_rxns"/>
  <database>
    <string_value lines="1">https://zenodo.org/record/6350797/files/fo_sio2_db.tar.gz</string_value>
  </database>
  <reference>
    <string_value lines="1">fo_sio2/notebooks/Generate_fo_sio2_poly_linear_rxn.ipynb</string_value>
  </reference>
  <phase name="Liquid"/>
  <phase name="Olivine"/>
  <phase name="Orthopyroxene"/>
  <phase name="Silica_polymorph"/>
  <reaction_rate name="R">
    <variable name="T">
      <rank name="Scalar"/>
      <units>K</units>
    </variable>
    <variable name="P">
      <rank name="Scalar"/>
      <units>bar</units>
    </variable>
    <variable name="C">
      <rank name="Matrix">
        <n_rows>N</n_rows>
        <n_cols>Kmax</n_cols>
      </rank>
      <units>None</units>
    </variable>
    <variable name="Phi">
      <rank name="Vector">
        <size>N</size>
      </rank>
      <units>%</units>
    </variable>
    <function name="A">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
        <variable name="C">
          <rank name="Matrix">
            <n_rows>N</n_rows>
            <n_cols>Kmax</n_cols>
          </rank>
          <units>None</units>
        </variable>
      </type>
    </function>
    <function name="rho">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
      </type>
    </function>
  </reaction_rate>
  <global_parameters>
    <parameter name="T0">
      <rank name="Scalar">
        <value>
          <real_value rank="0">1750</real_value>
        </value>
        <units>
          <string_value lines="1">'K'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="R">
      <rank name="Scalar">
        <value>
          <real_value rank="0">8.314</real_value>
        </value>
        <units>
          <string_value lines="1">'J/(mol K)'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="r0_xtal">
      <rank name="Scalar">
        <value>
          <real_value rank="0">1</real_value>
        </value>
        <units>
          <string_value lines="1">'kg/m^2/s'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="r0_melt">
      <rank name="Scalar">
        <value>
          <real_value rank="0">1</real_value>
        </value>
        <units>
          <string_value lines="1">'kg/m^2/s'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="Stot">
      <rank name="Scalar">
        <value>
          <real_value rank="0">1</real_value>
        </value>
        <units>
          <string_value lines="1">'m^-1'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="eps">
      <rank name="Scalar">
        <value>
          <real_value rank="0">0.1</real_value>
        </value>
        <units>
          <string_value lines="1">'None'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="M0">
      <rank name="Scalar">
        <value>
          <real_value rank="0">100</real_value>
        </value>
        <units>
          <string_value lines="1">'g/mol'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="M_fo">
      <rank name="Scalar">
        <value>
          <real_value rank="0">140.691</real_value>
        </value>
        <units>
          <string_value lines="1">'g/mol'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="M_en">
      <rank name="Scalar">
        <value>
          <real_value rank="0">100.387</real_value>
        </value>
        <units>
          <string_value lines="1">'g/mol'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="M_qz">
      <rank name="Scalar">
        <value>
          <real_value rank="0">60.083</real_value>
        </value>
        <units>
          <string_value lines="1">'g/mol'</string_value>
        </units>
      </rank>
    </parameter>
  </global_parameters>
  <reaction name="Ol_melting">
    <reactant name="Fo_Ol" phase="Olivine" endmember="Forsterite_berman"/>
    <product name="Fo_Lq" phase="Liquid" endmember="Forsterite_xmelts"/>
    <expression>
      <string_value type="code" language="python" lines="20">Ol_melting = (
Piecewise((M0*Stot*r0_melt*exp(-T0/T)*A[0, 0]*Phi[1, 0]/(M_fo*R*T*(eps
+ Phi[1, 0])), A[0, 0] &gt;= 0), (M0*Stot*r0_xtal*exp(-T0/T)*A[0,
0]*Phi[0, 0]/(M_fo*R*T*(eps + Phi[0, 0])), True))
)</string_value>
    </expression>
  </reaction>
  <reaction name="Opx_melting">
    <reactant name="En_Opx" phase="Orthopyroxene" endmember="Orthoenstatite_berman"/>
    <product name="Fo_Lq" phase="Liquid" endmember="Forsterite_xmelts"/>
    <product name="Si_lq" phase="Liquid" endmember="Quartz4_liquid"/>
    <expression>
      <string_value type="code" language="python" lines="20">Opx_melting = (
Piecewise((M0*Stot*r0_melt*exp(-T0/T)*A[1, 0]*Phi[2, 0]/(M_en*R*T*(eps
+ Phi[2, 0])), A[1, 0] &gt;= 0), (M0*Stot*r0_xtal*exp(-T0/T)*A[1,
0]*Phi[0, 0]/(M_en*R*T*(eps + Phi[0, 0])), True))
)</string_value>
    </expression>
  </reaction>
  <reaction name="Si_melting">
    <reactant name="Si_Si" phase="Silica_polymorph" endmember="Silica_polymorph_berman"/>
    <product name="Si_lq" phase="Liquid" endmember="Quartz4_liquid"/>
    <expression>
      <string_value type="code" language="python" lines="20">Si_melting = (
Piecewise((M0*Stot*r0_melt*exp(-T0/T)*A[2, 0]*Phi[3, 0]/(M_qz*R*T*(eps
+ Phi[3, 0])), A[2, 0] &gt;= 0), (M0*Stot*r0_xtal*exp(-T0/T)*A[2,
0]*Phi[0, 0]/(M_qz*R*T*(eps + Phi[0, 0])), True))
)</string_value>
    </expression>
  </reaction>
</reaction_options>

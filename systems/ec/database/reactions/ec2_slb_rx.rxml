<?xml version='1.0' encoding='utf-8'?>
<reaction_options>
  <name name="ec2_slb_rx"/>
  <database>
    <string_value lines="1">file://../database/tcg_slb_database.tar.gz</string_value>
  </database>
  <reference>
    <string_value lines="1">generate_reactions</string_value>
  </reference>
  <phase name="Clinopyroxene_slb_ph"/>
  <phase name="Orthopyroxene_slb_ph"/>
  <phase name="Garnet_slb_ph"/>
  <phase name="Feldspar_slb_ph"/>
  <phase name="Quartz_slb_ph"/>
  <reaction_rate name="R">
    <variable name="T">
      <rank name="Scalar"/>
      <units>K</units>
    </variable>
    <variable name="P">
      <rank name="Scalar"/>
      <units>bar</units>
    </variable>
    <variable name="C">
      <rank name="Matrix">
        <n_rows>N</n_rows>
        <n_cols>Kmax</n_cols>
      </rank>
      <units>None</units>
    </variable>
    <variable name="Phi">
      <rank name="Vector">
        <size>N</size>
      </rank>
      <units>%</units>
    </variable>
    <function name="A">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
        <variable name="C">
          <rank name="Matrix">
            <n_rows>N</n_rows>
            <n_cols>Kmax</n_cols>
          </rank>
          <units>None</units>
        </variable>
      </type>
    </function>
    <function name="rho">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
      </type>
    </function>
  </reaction_rate>
  <global_parameters>
    <parameter name="T0">
      <rank name="Scalar">
        <value>
          <real_value rank="0">2000</real_value>
        </value>
        <units>
          <string_value lines="1">'K'</string_value>
        </units>
      </rank>
    </parameter>
    <parameter name="R">
      <rank name="Scalar">
        <value>
          <real_value rank="0">8.31446261815324</real_value>
        </value>
        <units>
          <string_value lines="1">'J/mol/K'</string_value>
        </units>
      </rank>
    </parameter>
  </global_parameters>
  <reaction name="Albi_JadeQuar">
    <reactant name="Albi_Feld" phase="Feldspar_slb_ph" endmember="Albite_slb_em"/>
    <product name="Jade_Clin" phase="Clinopyroxene_slb_ph" endmember="Jadeite_slb_em"/>
    <product name="Quar_Quar" phase="Quartz_slb_ph" endmember="Quartz_slb_em"/>
    <expression>
      <string_value type="code" language="python" lines="20">Albi_JadeQuar = (
Piecewise((exp(-T0/T)*A[0, 0]*Phi[3, 0]/(R*T), A[0, 0] &gt;= 0),
(exp(-T0/T)*A[0, 0]*Phi[0, 0]*Phi[4, 0]/(R*T), True))
)</string_value>
    </expression>
  </reaction>
  <reaction name="AnorEnst_DiopPyroQuar">
    <reactant name="Anor_Feld" phase="Feldspar_slb_ph" endmember="Anorthite_slb_em"/>
    <reactant name="Enst_Orth" phase="Orthopyroxene_slb_ph" endmember="Enstatite_slb_em"/>
    <product name="Diop_Clin" phase="Clinopyroxene_slb_ph" endmember="Diopside_slb_em"/>
    <product name="Pyro_Garn" phase="Garnet_slb_ph" endmember="Pyrope_slb_em"/>
    <product name="Quar_Quar" phase="Quartz_slb_ph" endmember="Quartz_slb_em"/>
    <expression>
      <string_value type="code" language="python" lines="20">AnorEnst_DiopPyroQuar = (
Piecewise((exp(-T0/T)*A[1, 0]*Phi[1, 0]*Phi[3, 0]/(R*T), A[1, 0] &gt;=
0), (exp(-T0/T)*A[1, 0]*Phi[0, 0]*Phi[2, 0]*Phi[4, 0]/(R*T), True))
)</string_value>
    </expression>
  </reaction>
</reaction_options>

  <reaction name="KyanEnst_QuarMgTs">
    <reactant name="Kyan_Kyan" phase="Kyanite_slb_ph" endmember="Kyanite_slb_em"/>
    <reactant name="Enst_Orth" phase="Orthopyroxene_slb_ph" endmember="Enstatite_slb_em"/>

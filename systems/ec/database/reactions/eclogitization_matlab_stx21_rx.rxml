    <product name="MgTs_Orth" phase="Orthopyroxene_slb_ph" endmember="MgTschermaks_slb_em"/>
    <expression>
      <string_value type="code" language="python" lines="20">KyanEnst_QuarMgTs = (
Piecewise((exp(-T0/T)*A[0, 0]*Phi[1, 0]*Phi[5, 0]/(R*T), A[0, 0] &gt;=
0), (exp(-T0/T)*A[0, 0]*Phi[1, 0]*Phi[2, 0]/(R*T), True))

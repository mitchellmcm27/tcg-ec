    </variable>
    <function name="A">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
        <variable name="C">
          <rank name="Matrix">
            <n_rows>N</n_rows>
            <n_cols>Kmax</n_cols>
          </rank>
          <units>None</units>
        </variable>
      </type>
    </function>
    <function name="rho">
      <type name="external">
        <rank name="Vector">
          <size>J</size>
        </rank>
        <variable name="T">
          <rank name="Scalar"/>
          <units>K</units>
        </variable>
        <variable name="P">
          <rank name="Scalar"/>
          <units>bar</units>
        </variable>
      </type>
    </function>

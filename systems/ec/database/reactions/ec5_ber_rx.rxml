    </parameter>
    <parameter name="R">
      <rank name="Scalar">
        <value>
          <real_value rank="0">8.31446261815324</real_value>
        </value>
        <units>
          <string_value lines="1">'J/mol/K'</string_value>
        </units>
      </rank>
    </parameter>
  </global_parameters>
  <reaction name="Albi_JadeB_Qu">
    <reactant name="Albi_Plag" phase="Plagioclase_ber_ph" endmember="Albite_ber_em"/>
    <product name="Jade_Clin" phase="Clinopyroxene_ber_ph" endmember="Jadeite_ber_em"/>
    <product name="B_Qu_Quar" phase="Quartz_ber_ph" endmember="B_Quartz_ber_em"/>
    <expression>
      <string_value type="code" language="python" lines="20">Albi_JadeB_Qu = (
Piecewise((exp(-T0/T)*A[0, 0]*Phi[3, 0]/(R*T), A[0, 0] &gt;= 0),
(exp(-T0/T)*A[0, 0]*Phi[0, 0]*Phi[4, 0]/(R*T), True))
)</string_value>
    </expression>
  </reaction>
  <reaction name="AnorOrth_GrosPyroB_Qu">

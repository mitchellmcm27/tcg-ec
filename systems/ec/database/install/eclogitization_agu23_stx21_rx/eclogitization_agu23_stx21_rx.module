prepend-path  PYTHONPATH		     /workspaces/tcg-ec/systems/ec/database/install/eclogitization_agu23_stx21_rx/lib/python3.8/site-packages
prepend-path  LD_LIBRARY_PATH	     /workspaces/tcg-ec/systems/ec/database/install/eclogitization_agu23_stx21_rx/lib
prepend-path  CPLUS_INCLUDE_PATH  /workspaces/tcg-ec/systems/ec/database/install/eclogitization_agu23_stx21_rx/include:/workspaces/tcg-ec/systems/ec/database/install/eclogitization_agu23_stx21_rx/include/coder

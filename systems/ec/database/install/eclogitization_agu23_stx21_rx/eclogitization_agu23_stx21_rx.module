#%Module1.0
proc ModulesHelp { } {
        global dotversion

        puts stderr "	Sets environment for custom thermocodegen libraries."
}
module-whatis "Sets environment for interaction with custom thermocodegen libraries"


# Standard paths
setenv	  LIB_PREFIX				/workspaces/tcg-ec/systems/ec/database/install/eclogitization_agu23_stx21_rx
